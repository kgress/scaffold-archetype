<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xmlns="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0 http://maven.apache.org/xsd/archetype-descriptor-1.1.0.xsd"
  name="${artifactId}">
    <modules>
        <module id="core" dir="core" name="core">
            <fileSets>
                <fileSet filtered="true" packaged="true" encoding="UTF-8">
                    <directory>src/main/java</directory>
                </fileSet>
                <fileSet filtered="true" packaged="true" encoding="UTF-8">
                    <directory>src/test/java</directory>
                </fileSet>
            </fileSets>
        </module>

        <module id="environment" dir="environment" name="environment">
            <fileSets>
                <fileSet filtered="true" packaged="true" encoding="UTF-8">
                    <directory>src/main/java</directory>
                </fileSet>
                <fileSet encoding="UTF-8">
                    <directory>src/main/resources</directory>
                </fileSet>
            </fileSets>
        </module>
    </modules>

    <fileSets>
        <!--Root Files-->
        <fileSet filtered="true" encoding="UTF-8">
            <directory/>
            <includes>
                <include>.gitignore</include>
                <include>README.MD</include>
            </includes>
        </fileSet>
    </fileSets>
</archetype-descriptor>